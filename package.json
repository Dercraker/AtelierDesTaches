{
  "name": "atelier-des-taches",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "patch": "pnpm version patch --force --no-git-tag-version",
    "minor": "pnpm version minor --force --no-git-tag-version",
    "prerelease": "pnpm version prerelease --force --no-git-tag-version",
    "major": "pnpm version major --force --no-git-tag-version",
    "build": "next build",
    "start": "next start",
    "ts": "tsc --noEmit",
    "postinstall": "prisma generate",
    "clean": "next lint --fix && tsc --noEmit && prettier --write .",
    "knip": "knip"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.12.0",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^23.7.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}

