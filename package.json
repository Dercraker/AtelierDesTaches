{
  "name": "atelier-des-taches",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "patch": "pnpm version patch --force --no-git-tag-version",
    "minor": "pnpm version minor --force --no-git-tag-version",
    "prerelease": "pnpm version prerelease --force --no-git-tag-version",
    "major": "pnpm version major --force --no-git-tag-version",
    "build": "next build",
    "start": "next start",
    "ts": "tsc --noEmit",
    "postinstall": "prisma generate",
    "clean": "next lint --fix && tsc --noEmit && prettier --write .",
    "knip": "knip"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.1.6"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "@eslint/eslintrc": "^3"
  }
}
